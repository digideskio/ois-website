        script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body
    .navbar.navbar-inverse.navbar-fixed-top(role='navigation')
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/')
        .collapse.navbar-collapse
          ul.nav.navbar-nav
            li
              a(href='/') <img src='http://data.oxfordindiasociety.org.uk/images/ois-logo.jpg'>
            li
              a(href='/about') About
            li
              a(href='/events') Events
            li
              a(href='http://policyblog.oxfordindiasociety.org.uk') Policy Blog
            li
              a(href='/contact') Contact
            li
              a(href='/election') Election
        // /.nav-collapse
    block main
      .container.main
        block body
            p Nothing to see here
